            <string>/// @description Insert description here
// You can write your code in this editor
if keyboard_check(vk_escape)
    game_end()



touch = mouse_check_button(mb_left)

keyboard_y = lerp(keyboard_y,0,0.5)

if touch
{
if touchlongtimer&gt;touch_stop_delay
touchlong = mouse_check_button(mb_left)
else
touchlong = mouse_check_button_pressed(mb_left)
touchlongtimer++
}
else
{
touchlong = 0
touchlongtimer=0
}

letters = "" //what the letters that been placed

if togglevar //keyboard_keys = togglevar ? mode2 : mode1
    keyboard_keys=mode2
else
    keyboard_keys=mode1

var dis = 6
var width = 155
var mousex = device_mouse_x_to_gui(0)
var mousey = device_mouse_y_to_gui(0)

draw_sprite(spr_key_background,0,keyboard_x,keyboard_y)

var stag = 0

for (var i = 0; i&lt;12; i++)
{
var pos = (width+dis) * i-i
keyboard_draw_key(3+pos+keyboard_x,527+keyboard_y,width,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)
stag++
} 

var dis = 6
var exdis = 0
var width = 169

for (var i = 0; i&lt;11; i++)
{
var pos = (width+dis) * i-i + exdis*i
keyboard_draw_key(3+pos+keyboard_x,666+keyboard_y,width,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)
stag++
if i=1 exdis++
if i=4 exdis-=1/5
if i=8 {width-=2 exdis+=2-1/9 }
}

var dis = 7
var exdis = 0
var width = 168

for (var i = 0; i&lt;7; i++)
{
var pos = (width+dis) * i-i + exdis*i
keyboard_draw_key(353+pos+keyboard_x,805+keyboard_y,width,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)
stag++
if i=1 exdis++
}

var res = keyboard_draw_key(3+keyboard_x,805+keyboard_y,343,keyboard_keys[stag],mousex,mousey,c_ltgray,c_dkgray,1,alp,togglevar) // Shift
togglevar = res[1]
if res[2]
letters = ""

stag++

var res = keyboard_draw_key(1575+keyboard_x,805+keyboard_y,343,keyboard_keys[stag],mousex,mousey,c_ltgray,c_dkgray,0,alp) // Backspace
if res[2]
{letters = "" TEXT = string_delete(TEXT,string_length(TEXT),1)}

stag++

keyboard_draw_key(484+keyboard_x,943+keyboard_y,186,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)// /
stag++

var res = keyboard_draw_key(676+keyboard_x,943+keyboard_y,377,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)// Space
if res[2]
letters = " "
stag++

if togglevar
    var colorr = c_ltgray
else
    var colorr = c_white

var res = keyboard_draw_key(1060+keyboard_x,943+keyboard_y,185,keyboard_keys[stag],mousex,mousey, colorr,c_dkgray,0,alp)// , or Tab
if res[2] &amp;&amp; togglevar
{
letters="       "
}

stag++

keyboard_draw_key(1251+keyboard_x,943+keyboard_y,186,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)// .
stag++

var res = keyboard_draw_key(1443+keyboard_x,943+keyboard_y,474,keyboard_keys[stag],mousex,mousey,c_aqua,c_white,0,alp)// Go
if res[2]
{
letters = ""
instance_destroy()
}
stag++

var res = keyboard_draw_key(3+keyboard_x,943+keyboard_y,475,keyboard_keys[stag],mousex,mousey,c_ltgray,c_dkgray,0,alp)// Enter
if res[2]
letters = "\n"
stag++

var dis = 6
var width = 155

for (var i = 0; i&lt;12; i++) // 1 - 90!?
{
var pos = (width+dis) * i-i
keyboard_draw_key(3+pos+keyboard_x,388+keyboard_y,width,keyboard_keys[stag],mousex,mousey,c_white,c_dkgray,0,alp)
stag++
}

draw_set_font(-1)
TEXT += letters

